const { assert, expect } = require('chai')

import {asn1_u8_encode, asn1_u8_decode, asn1_u8_decode_many} from 'asn1-codec/esm/index.mjs'
const umd_or_module = require('asn1-codec')

describe @ 'smoke', @::
  it @ 'esm basic', @::
    assert.equal @ 'function', typeof asn1_u8_encode
    assert.equal @ 'function', typeof asn1_u8_decode
    assert.equal @ 'function', typeof asn1_u8_decode_many

  it @ 'commonjs / web', @::
    assert.equal @ 'function', typeof umd_or_module.asn1_u8_encode
    assert.equal @ 'function', typeof umd_or_module.asn1_u8_decode
    assert.equal @ 'function', typeof umd_or_module.asn1_u8_decode_many

